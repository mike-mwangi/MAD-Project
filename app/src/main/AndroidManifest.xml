<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.madproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MADPROJECT.Blue">
        <activity
            android:name=".SingleAudioActivity"
            android:exported="true" />
        <activity android:name=".ProfileActivity"></activity>
        <activity
            android:name=".BlogPostActivity"
            android:parentActivityName=".HomeActivity"
            android:theme="@style/MADPROJECT.Blue" /> <!-- <activity android:name=".HomeActivity"></activity> -->
        <!-- <activity android:name=".HomeActivity"></activity> -->
        <activity android:name=".Landing_Page" />
        <activity android:name=".Login" />
        <activity
            android:name=".PostingActivity"
            android:theme="@style/MADPROJECT.Blue" />
        <activity android:name=".Sign_Up"></activity>
        <activity
            android:name=".HomeActivity"
            android:theme="@style/MADPROJECT.Blue" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/MADPROJECT.Blue">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <meta-data
            android:name="fontProviderRequests"
            android:value="Rubik" />
    </application>

</manifest>